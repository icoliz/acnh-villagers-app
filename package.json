{
  "name": "acnh-villagers-app",
  "version": "1.0.0",
  "description": "animal crossing villagers application",
  "main": "index.js",
  "dependencies": {
    "@lion/ajax": "^0.16.0",
    "@lion/button": "^0.18.0",
    "@lion/input": "^0.18.0",
    "@lion/localize": "^0.25.0",
    "@lion/select-rich": "^0.31.0",
    "@open-wc/scoped-elements": "^2.0.0-next.6",
    "@vaadin/router": "^1.7.4",
    "lit-element": "^3.2.0"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/testing": "^3.0.0-next.5",
    "@web/dev-server": "^0.1.31",
    "@web/dev-server-storybook": "^0.5.1",
    "@web/test-runner": "^0.13.29",
    "@web/test-runner-visual-regression": "^0.6.5"
  },
  "scripts": {
    "start": "web-dev-server --node-resolve --open --watch",
    "storybook": "wds --config .web-server-storybook/web-server-storybook.config.mjs",
