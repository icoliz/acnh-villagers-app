    "test:watch": "web-test-runner ./src/**/test/*test.js --node-resolve --watch --coverage",
    "test:visual": "web-test-runner --config .web-test-runner/web-test-runner.config.mjs",
    "test:visual-update": "web-test-runner --config .web-test-runner/web-test-runner.config.mjs --update-visual-baseline"
  },
